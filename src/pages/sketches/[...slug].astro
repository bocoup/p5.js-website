---
import SketchLayout from "@/src/layouts/SketchLayout.astro";
import { getCurationSketches } from "../_api";

export async function getStaticPaths() {
  const sketches = await getCurationSketches();
  return sketches.map((entry) => ({
    params: { slug: entry.visualID },
    props: {},
  }));
}

const { slug } = Astro.params;
---

<SketchLayout sketchId={slug} />
